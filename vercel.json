{
    "name": "portfolio-tkpatel",
    "version": 2,
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "https://my-server-khaki.vercel.app/api/portfolio/$1",
            "methods": ["GET", "POST", "PUT", "DELETE"]
        },
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
}
